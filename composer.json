{
    "name": "avikuloff/laravel-quickfilters",
    "description": "Quick filters for eloquent query builder",
    "keywords": [
        "filters",
        "search",
        "laravel",
        "eloquent",
        "builder"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Vikulov",
            "email": "avikuloff@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "mpociot/pipeline": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Avikuloff\\QuickFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avikuloff\\QuickFilters\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avikuloff\\QuickFilters\\QuickFiltersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
